<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.5.0rc1//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/2.5.0rc1/distro-source/core/src/gwt-module.dtd">
<module rename-to='core'>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="org.fusesource.restygwt.RestyGWT"/>
    <inherits name="org.syswin.fences.core"/>

    <inherits name="com.google.gwt.inject.Inject"/>
    <!-- Inheriting 'Debug' on the next line makes the features available. -->
    <inherits name='com.google.gwt.debug.Debug'/>



    <!--Bootstrap-->
    <inherits name ="com.github.gwtbootstrap.Bootstrap"/>

    <!--Add logging module-->
    <inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.logLevel" value="FINEST"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/>
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />

    <!-- Disable for production by uncommenting the next line -->
    <!-- <set-property name="gwt.enableDebugId" value="false"/> -->

    <!-- Specify the paths for translatable code -->
    <source path="client"/>




    <!--&lt;!&ndash; Specify the paths for translatable code &ndash;&gt;-->
    <!--<source path='webapp'/>-->
    <!--<source path='core'/>-->

    <!--&lt;!&ndash; Specify the app entry point class. &ndash;&gt;-->
    <!--<entry-point class='org.syswin.fences.client.App'/>-->



</module>
